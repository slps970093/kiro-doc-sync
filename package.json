{
  "name": "kiro-doc-sync",
  "version": "1.0.3",
  "description": "CLI tool to sync Kiro documentation files from remote Git repositories",
  "main": "dist/index.js",
  "bin": {
    "kiro-doc-sync": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "postinstall": "node scripts/postinstall.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "kiro",
    "sync",
    "documentation",
    "git",
    "cli",
    "sparse-checkout"
  ],
  "author": "Yu-Hsien, Chou <slps970093@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/slps970093/kiro-doc-sync"
  },
  "bugs": {
    "url": "https://github.com/slps970093/kiro-doc-sync/issues"
  },
  "homepage": "https://github.com/slps970093/kiro-doc-sync#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "scripts",
    "README.md",
    "README.en.md",
    "README.zh.md",
    "LICENSE"
  ],
  "dependencies": {
    "commander": "^11.0.0",
    "glob": "^10.0.0",
    "simple-git": "^3.20.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  }
